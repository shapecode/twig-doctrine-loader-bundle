{
    "name": "shapecode/twig-doctrine-loader-bundle",
    "description": "Adds a doctrine loader to twig",
    "keywords": [
        "symfony",
        "shapecode",
        "twig",
        "template",
        "doctrine",
        "doctrine loader",
        "loader",
        "bundle"
    ],
    "type": "symfony-bundle",
    "license": "MIT",
    "homepage": "https://github.com/shapecode/entitytwig-doctrine-loader-bundle",
    "support": {
        "email": "support@shapecode.de",
        "issues": "https://github.com/shapecode/entitytwig-doctrine-loader-bundle/issues",
        "source": "https://github.com/shapecode/entitytwig-doctrine-loader-bundle/releases",
        "wiki": "https://github.com/shapecode/entitytwig-doctrine-loader-bundle/wiki"
    },
    "authors": [
        {
            "name": "Nikita Loges",
            "homepage": "http://nikita-loges.de",
            "email": "develop@nikita-loges.de"
        }
    ],
    "require": {
        "php": "~5.5|~7.0",

        "symfony/dependency-injection": "~2.7|~3.0",
        "symfony/http-kernel": "~2.7|~3.0",
        "symfony/config": "~2.7|~3.0",
        "symfony/twig-bundle": "~2.7|~3.0",

        "doctrine/doctrine-bundle": "~1.6",
        "doctrine/orm": "~2.5",

        "twig/twig": "~1.18|~2.0"
    },
    "autoload": {
        "psr-4": {
            "Shapecode\\Bundle\\TwigDoctrineLoaderBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shapecode\\Bundle\\TwigDoctrineLoaderBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-release/1.0": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
